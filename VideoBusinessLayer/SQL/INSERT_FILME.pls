create or replace NONEDITIONABLE PROCEDURE insert_filme(
    ID FILMES.ID%TYPE,
    TITULO FILMES.TITULO%TYPE,
    ANO FILMES.ANO%TYPE,
    IMDB_URL FILMES.IMDB_URL%TYPE
)
IS
BEGIN
  INSERT INTO FILMES (TITULO, ANO, IMDB_URL ) VALUES (TITULO, ANO, IMDB_URL );
  COMMIT;
EXCEPTION
   WHEN OTHERS THEN
      dbms_output.put_line( SQLERRM );
END;